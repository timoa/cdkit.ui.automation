{
  "name": "cdkit.ui.automation",
  "version": "0.8.1",
  "description": "Appium infrastructure to run Appium tests on iOS simulators and Genymotion emulators.",
  "main": "cli.js",
  "private": true,
  "scripts": {
    "pretest": "./node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "test:coverage": "./node_modules/.bin/nyc npm test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:timoa/cdkit.ui.automation.git"
  },
  "keywords": [
    "appium",
    "titanium",
    "mobile"
  ],
  "author": "Wilson Luu",
  "contributors": [
    {
      "name": "Damien Laureaux",
      "url": "https://timoa.com"
    }
  ],
  "license": "ISC",
  "homepage": "https://github.com/timoa/cdkit.ui.automation#readme",
  "dependencies": {
    "appium": "1.20.2",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "commander": "3.0.2",
    "mocha": "9.2.2",
    "q": "2.0.3",
    "snyk": "1.685.0",
    "wd": "1.13.0",
    "winston": "3.3.4"
  },
  "engines": {
    "node": ">=10.0"
  },
  "os": [
    "linux",
    "win32",
    "darwin"
  ],
  "devDependencies": {
    "acorn": "6.4.2",
    "acorn-jsx": "5.3.2",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "espree": "4.1.0",
    "nyc": "14.1.1"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "config/*.js",
      "helpers/*.js",
      "lib/*.js",
      "cli.js"
    ],
    "exclude": [
      "test/*.spec.js",
      "coverage/**",
      "ui-tests/**/*.js"
    ],
    "all": true
  },
  "snyk": true
}
