{
  "name": "cdkit.ui.automation",
  "version": "0.8.1",
  "description": "Appium infrastructure to run Appium tests on iOS simulators and Genymotion emulators.",
  "main": "cli.js",
  "private": true,
  "snyk": true,
  "scripts": {
    "pretest": "./node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "test:coverage": "./node_modules/.bin/nyc npm test",
    "snyk-protect": "snyk-protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:timoa/cdkit.ui.automation.git"
  },
  "keywords": [
    "appium",
    "titanium",
    "mobile"
  ],
  "author": "Wilson Luu",
  "contributors": [
    {
      "name": "Damien Laureaux",
      "url": "https://timoa.com"
    }
  ],
  "license": "ISC",
  "homepage": "https://github.com/timoa/cdkit.ui.automation#readme",
  "dependencies": {
    "@snyk/protect": "1.1029.0",
    "appium": "1.22.3",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "commander": "3.0.2",
    "mocha": "10.0.0",
    "q": "2.0.3",
    "wd": "1.14.0",
    "winston": "3.8.2"
  },
  "engines": {
    "node": ">=16.0",
    "npm": ">=8.6.0"
  },
  "os": [
    "linux",
    "win32",
    "darwin"
  ],
  "devDependencies": {
    "acorn": "8.8.0",
    "acorn-jsx": "5.3.2",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "espree": "9.4.0",
    "nyc": "15.1.0"
  }
}
